<h1>Create an Account</h1>
<%= form_with url: '/users', local: true do |f| %>
  <%= f.label :email, 'Email' %>
  <%= f.text_field :email %>

  <%= f.label :password, 'Password' %>
  <%= f.password_field :password %>  

  <%= f.label :password_confirmation, 'Confirm Password' %>
  <%= f.password_field :password_confirmation %>

  <%= f.submit 'Submit' %>
<% end %>
